
@{
    ViewBag.Title = "Report2";
}


@*
    This report is dedicated to the static dropdown basis data segregated    
*@
<h2>Report1 is here</h2>
@using (Html.BeginForm())
{
    <div class="mt-3">
        <select class="form-control" name="Agreement_Status" onchange="this.form.submit();">
            <option value="">Select Status</option>
            <option value="Active">Active</option>
            <option value="Completed">Completed</option>
        </select>
    </div>
}
<table class="table table-striped">
    <thead>
        <tr class="fw-semibold">
            <td>Tenant Name</td>
            <td>House_Address</td>
            <td>Start Date</td>
            <td>End Date</td>
            <td>Deposit Amount</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.tenantname</td>
                <td>@item.House_Address</td>
                <td>@item.StartDate</td>
                <td>@item.EndDate</td>
                <td>@item.Deposit_Amount</td>
            </tr>
        }
    </tbody>
</table>

